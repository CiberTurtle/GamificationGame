config_version=5

[application]

config/name="Poultry Pounder"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.0", "Mobile")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
boot_splash/image="res://icon.png"
